{
    "env": {
        "es6": true,
        "node": true
    },
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "extends": ["eslint:recommended", "plugin:prettier/recommended"],
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": "error",
        "no-var": "error",
        "no-console": ["warn", { "allow": ["warn", "error", "info"] }],
        "eqeqeq": "warn",
        "no-unused-vars": "warn"
    }
}
